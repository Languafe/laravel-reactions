{
    "name": "languafe/laravel-reactions",
    "description": "Add reactions to Eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Erik Langhaug"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Languafe\\Reactions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Languafe\\Reactions\\ReactionsServiceProvider"
            ],
            "aliases": {
                "Reactions": "Languafe\\Reactions\\Facades\\Reactions"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
